{
    "version": "0.4.0",
    "name": "uyanik13/dro-seo-analyzer",
    "description": "DRO SEO Analyzer",
    "keywords": ["SEO", "Analyzer", "onpage", "on page", "on", "page", "seo"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ogur Uyanik",
            "email": "ogur@dijitalreklam.org"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/support": "~5.0|6.*|7.*|8.*",
        "guzzlehttp/guzzle": "~6.0|7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*|6.*|8.*",
        "php-coveralls/php-coveralls": "~2.1",
        "ivopetkov/html5-dom-document-php": "1.1.x-dev"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "DRO\\SeoAnalyzer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DRO\\SeoAnalyzer\\SeoServiceProvider"
            ],
            "aliases": {
                "Seo": "DRO\\SeoAnalyzer\\SeoFacade"
            }
        }
    }
}
